{
    "name": "samovar/symfony-console-skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "samovar",
            "email": "prototype.denis@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.3||^7.2",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/config": "^4.3",
        "symfony/filesystem": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^4.3"
    },
    "config": {
        "sort-packages": true
    }
}
