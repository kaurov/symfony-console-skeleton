{
    "name": "samovar/symfony-console-skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "samovar",
            "email": "prototype.denis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    }
}
