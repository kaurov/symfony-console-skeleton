{
  "alias": "box.phar",
  "chmod": "0755",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php",
    "KevinGH\\Box\\Compactor\\PhpScoper",
    "KevinGH\\Box\\Compactor\\Json"
  ],
  "directories": [
    "src",
    "vendor"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "Tests",
        "tests"
      ],
      "in": "vendor"
    }
  ],
  "git-commit-short": "git_commit_short",
  "main": "bin/console",
  "output": "build/app.phar",
  "stub": true
}
